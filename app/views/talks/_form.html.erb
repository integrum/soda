<% form_for [@meeting,@talk], :html => {:multipart => true} do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :user_id %>
    <%= f.select :user_id, user_drop_down_options %>
    
    <%= f.label :uploaded_data %>
    <%= f.file_field :uploaded_data %>
    
    <%= f.submit "Save" %>
<% end %>