<% form_for @meeting do |f| %>
    <%= f.label :location %>
    <%= f.text_field :location%>
    
    <%= f.label :meeting_keyword_list %>
    <%= f.text_field :meeting_keyword_list %>
    
    <%= f.label :occurs_at %>
    <%= f.datetime_select :occurs_at %>
    
    <%= f.submit "Save" %>
<% end %>